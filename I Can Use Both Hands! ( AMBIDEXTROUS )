/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		while(t-->0){
		    double l = s.nextInt();
		    double r = s.nextInt();
		    double m = s.nextInt();
		    double page = Math.ceil(m/l);
		    
		    System.out.println((int)(page + (m/r)));
		}
	}
}
